 StyleBox["  ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], 
     RowBox[{"n", "-", "k", "-", "j", "-", "1"}]], " ", 
    FractionBox[
     RowBox[{
      SubscriptBox["\[Phi]", 
       RowBox[{"n", "-", "k", "-", "j", "-", "1"}]], "(", 
      RowBox[{"x", " ", "\[CenterDot]", " ", "X"}], ")"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "-", "j", "+", "m", "-", "1"}], ")"}], "!"}]]}], 
