Cell[TextData[{
 StyleBox["Let x \[Element] ",
  FontWeight->"Bold"],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SuperscriptBox[
     TemplateBox[{},
      "Reals"], "d"], "  ", "and", " ", "X", "  ", "the", " ", "d"}], "-", 
   RowBox[{
