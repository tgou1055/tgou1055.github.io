82af220a064a"],

Cell[TextData[{
 StyleBox["and each matrix coefficient of ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   StyleBox[
    SubscriptBox["\[Pi]", "\[Lambda]"],
    FontColor->GrayLevel[0]], TraditionalForm]],
  FontWeight->"Bold",
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "0e4f2bcd-269b-418a-8eb5-d5bbecb1d5c5"],
 " ",
 StyleBox["can be calculated by the analytic formula:\n\n",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   StyleBox[
    SuperscriptBox[
     SubscriptBox["\[Pi]", 
      RowBox[{"j", ",", "k"}]], 
     RowBox[{"(", "\[Lambda]", ")"}]],
    FontSize->16], TraditionalForm]],
  FontColor->GrayLevel[0],ExpressionUUID->
  "cdbf3416-626b-42cf-97dc-ec8307b5e377"],
 StyleBox[" ",
  FontColor->GrayLevel[0]],
 "=",
 Cell[BoxData[
  FormBox[
   SqrtBox[
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Lambda]", "+", "1"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{"j", "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[Lambda]", "-", "j"}], ")"}], "!"}]}]]], TraditionalForm]],
  FontSize->18,ExpressionUUID->"ed10e383-bd29-4fd1-9f5a-e1d75aba051c"],
 StyleBox[" ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "1"], 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         OverscriptBox["\[Alpha]", "_"], "  ", 
         SuperscriptBox["e", 
          RowBox[{"\[ImaginaryI]", " ", "2", "\[Pi]", " ", "t"}]]}], "+", 
        "\[Beta]"}], ")"}], "k"], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          OverscriptBox["\[Beta]", "_"]}], " ", 
         SuperscriptBox["e", 
          RowBox[{"\[ImaginaryI]", " ", "2", "\[Pi]", " ", "t"}]]}], "+", " ",
         "\[Alpha]"}], ")"}], 
      RowBox[{"\[Lambda]", "-", "k"}]], "  ", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "\[ImaginaryI]2\[Pi]"}], " ", "j", " ", "t"}]], 
     RowBox[{"\[DifferentialD]", "t"}]}]}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"ca83a9d3-b5ed-4b6e-b605-6c39062199ca"]
