Cell[TextData[{
 StyleBox["We use non-commutative Kirillov Formula to calculate ",
  FontFamily->"Courier New",
  FontSize->16,
  FontWeight->"Bold",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Pi]", "\[Lambda]"], TraditionalForm]],
  FontWeight->"Bold",
  FontColor->GrayLevel[0],ExpressionUUID->
  "d66e8fd3-4f2d-4ad9-a8d4-aa08233fd612"],
 StyleBox[" of  SU(2) for highest weight \[Lambda] = 1, 2, 3. \n\nA matrix of \
SU(2) has representation\n\n\t\t",
  FontWeight->"Bold",
  FontColor->GrayLevel[0]],
 Cell[BoxData[
  TagBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"\[Alpha]", "\[Beta]"},
      {
       RowBox[{"-", 
        OverscriptBox["\[Beta]", "_"]}], 
