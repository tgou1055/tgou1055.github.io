Cell[TextData[{
 StyleBox["The orthonormal basis of the irreducible unitary representations \
of SU (2) is given by the homogeneous polynomials of two complex variables (\
\[Alpha], \[Beta]), that is\n\t\t\n\t\t",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    SqrtBox[
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[Lambda]", "+", "1"}], ")"}], "!"}], 
      RowBox[{
       RowBox[{"j", "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[Lambda]", "-", "j"}], ")"}], "!"}]}]]]}], 
   TraditionalForm]],
  FontSize->18,ExpressionUUID->"673a1021-8aca-4fcd-bbbb-38af170c2008"],
 StyleBox[" ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Alpha]", "j"], 
    SuperscriptBox["\[Beta]", 
     RowBox[{"\[Lambda]", "-", "j"}]]}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"34110c8d-0fe9-4698-bf69-f34937d2b7a9"],
 StyleBox["  :  0 \[LessSlantEqual] j \[LessSlantEqual] \[Lambda]",
  FontSize->18]
}], "Text",
 CellChangeTimes->{{3.91738765648365*^9, 3.917387787545216*^9}, {
